        <em:localized>
          <Description>
            <em:locale>de</em:locale>
            <em:name>Formlet</em:name>
            <em:description>Formulardaten als Lesezeichen speichern.</em:description>
            <em:translator>naitsirch</em:translator>
          </Description>
        </em:localized>

        <em:targetApplication>
            <Description>
                <!-- Firefox -->
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>11.0</em:minVersion>
                <em:maxVersion>36.0a2</em:maxVersion>
            </Description>
        </em:targetApplication>

    </Description>
</RDF>