<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">
        <em:type>2</em:type> <!-- 2 : Extension -->
        <em:id>formlet@preneta.pl</em:id>
        <em:version>1.1.1</em:version>
        <em:name>Formlet</em:name>
        <em:description>Saving form data to bookmarklets</em:description>
        <em:creator>Paweł Preneta (Jusuff)</em:creator>
        <em:optionsURL>chrome://formlet/content/options.xul</em:optionsURL>
        <em:optionsType>2</em:optionsType>
        <em:homepageURL>https://github.com/jusuff/Formlet</em:homepageURL>
        <em:iconURL>chrome://formlet/skin/formlet32.png</em:iconURL>
        <em:icon64URL>chrome://formlet/skin/formlet64.png</em:icon64URL>

        <em:localized>
          <Description>
            <em:locale>pl</em:locale>
            <em:name>Formlet</em:name>
            <em:description>Zapisuje formularze jako skryptozakładki.</em:description>
            <em:translator>Paweł Preneta (Jusuff)</em:translator>
